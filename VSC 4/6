
#printing the data type integer
age = 19
print(type(age))

#printing data type string
name = "Tracy"
print(type(name))

#printing data type boolean
student_status = True
print(type(student_status))


#printing data type float
student_balance = 1200000.48
print(type(student_balance))


#getting input from user
age_dynamic = int(input("What is your age? "))
print(type(age_dynamic))

name = input("What is your name? ")
print(name)

#
print("Age:", age_dynamic, "Type:", type(age_dynamic))

print("Student balance:", student_balance, "Type:", type(student_balance))
