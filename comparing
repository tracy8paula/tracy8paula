#Number 1
a=5
b=2.5
result= a+b
#printing out the result and type of the result
print(result)
print(type(result))

#Number 2
x=10
y=3.14
z=x*y
#printing the output and its data type
print(z)
print(type(z))

#Number 3
a=7
b=float(a)
#output of above code
print(b)
#data type of b
print(type(b))

#Number 4
a="42"
b= int(a)
#output of above code
print(a)
#datatype of b
print(type(b))

#NUMBER 5
a=15
b=3
c=a/b
#outputs
print(c)
print(type(c))
d=int(c)
#outputs about d
print(d)
print(type(d))
